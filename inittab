::respawn:/bin/cttyhack /bin/sh
::sysinit: -e "Welcome to Cutiepie Linux"
::restart:/sbin/init
::shutdown:printf -e "\nSyncing all file buffers."
::shutdown:sync
::shutdown:printf "Unmounting all filesystems."
::shutdown:umount -a -r
::ctrlaltdel:/sbin/reboot
tty2::respawn:/bin/sh
tty3::respawn:/bin/sh
tty4::respawn:/bin/sh
